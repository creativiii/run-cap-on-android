{
  "name": "run-cap-on-android",
  "version": "0.0.0-development",
  "description": "Builds a capacitor app on WSL and starts it on the Windows Android emulator.",
  "files": [
    "!lib/__tests__/**/*",
    "lib/**/*",
    "bin/**/*"
  ],
  "bin": {
    "run-cap-on-android": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "command": "ts-node src/cli.ts -a uk.co.staffscanner.test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/creativiii/run-cap-on-android.git"
  },
  "license": "MIT",
  "author": {
    "name": "Leonardo Petrucci",
    "email": "leonardo@ironeko.com",
    "url": "https://github.com/creativiii"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "capacitor",
    "cli",
    "ionic",
    "android"
  ],
  "bugs": {
    "url": "https://github.com/creativiii/run-cap-on-android/issues"
  },
  "homepage": "https://github.com/creativiii/run-cap-on-android#readme",
  "dependencies": {
    "commander": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "4.1.2",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.1.1",
    "husky": "^6.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^10.5.4",
    "ora": "^5.4.0",
    "prettier": "^2.2.1",
    "prompts": "^2.4.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "master"
    ]
  }
}
